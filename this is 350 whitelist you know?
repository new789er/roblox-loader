-- hub


local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local MarketplaceService = game:GetService("MarketplaceService")


local Whitelist = { "SecretsServiceAgent", "Ddemonred1", "1x1_1x235", "Alt_Giokid", "Nexorin8", "00de0l", "00de0l_test", "33qqr", "lolmy12114", "SkidEmpreor", "nika784895", "naszi0", "MoonToLua", "lnterminter123a", "kihank8557thu2" }


local function isWhitelisted(player)
	for _, name in ipairs(Whitelist) do
		if player.Name == name then
			return true
		end
	end
	return false
end


if not isWhitelisted(LocalPlayer) then
	local success, hasPass = pcall(function()
		return MarketplaceService:UserOwnsGamePassAsync(LocalPlayer.UserId, 1537160906)
	end)


	if not success or not hasPass then
		Players.LocalPlayer:Kick("I you're seeing this it's mean that your not whitelisted yet, you can buy whitelist or contact us here https://discord.gg/RthxwP579j")
	end
end



if not success or not hasPass then
	return
end
